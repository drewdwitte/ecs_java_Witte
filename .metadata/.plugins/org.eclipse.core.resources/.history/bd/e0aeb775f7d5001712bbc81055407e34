public class YourInitials
{
	public static void main(String[] args)
	{
		System.out.println("DDDD    EEEEE");
		System.out.println("D   D   E");
		System.out.println("D   D   E");
		System.out.println("D   D   EEE");
		System.out.println("D   D   E");
		System.out.println("D   D   E");
		System.out.println("DDDD    EEEEE");
		
	
	}
}
