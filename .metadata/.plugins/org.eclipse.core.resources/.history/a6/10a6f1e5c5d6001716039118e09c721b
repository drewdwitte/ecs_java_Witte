package Variables;

public class YourSchedule 
{
	public static void main(String[] args)
	{
		String Per1, Per2,Per3,Per4,Introtocalc,ECS,Weighttraining,Apush;
		
		Per1="Intro to Calculus";
		Per2="ECS";
		Per3="Weight training";
		Per4="Apush";
		Introtocalc="Escontrias";
		ECS="Mauro";
		Weighttraining="Lackey";
		Apush="Van Over";
		
		System.out.println("-----------------------------------------  ");
		System.out.println("|Per 1 | "+Per1+" |"+Introtocalc);
		System.out.println("|Per 2 | "+Per2+" |"+ECS);
		System.out.println("|Per 3 | "+Per3+" |"+Weighttraining);
		System.out.println("|Per 4 | "+Per4+" |"+Apush);
	}
}
