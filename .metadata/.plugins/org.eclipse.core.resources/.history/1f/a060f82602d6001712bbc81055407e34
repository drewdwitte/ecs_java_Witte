package Variables;

public class usingvariables
{
	public static void main(String[] args )
	{
		int room_number;
		double e;
		String subject;   
		
		room_number=113;
		e= 2.71828;
		String subject1= "Computer_Science";
		
		System.out.println("This is room#"+room_number);
		System.out.println("e is close to"+e);
		System.out.println("I am learning about"+subject1);
		
		
	}
	
	
}
